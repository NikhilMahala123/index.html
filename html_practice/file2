    <title>Page Title</title>
    <link rel="stylesheet" href="style.css">
    <a href="section"> blue </a>
</head>
<body>
    
      <h1 class="a1"> Heading1</h1>
      <h2 class="b2"> Heading1</h2>
      <h3 class="c3"> Heading1</h3>
      <h4 class="d4"> Heading1</h4>
      <div class="abc">
        <p>  this is para inside div</p>
      </div>
      <div id="section"   >
        <p> this is id para</p>
      </div>

</body>
</html>






